.secthree
  background-color #ED5A48
  font-family Muli
  color #FAFAFA
  padding 0
  margin-top 1rem

  &-h1
    font-size 1.5rem
    text-align center
    font-weight normal
    margin-bottom 1rem

  &-divtext
    display flex
    justify-content space-between

    &-ul
      margin-left 2rem
      font-size 1rem
      max-width 245px

      li
        margin-bottom 0.5rem

    &-picture
      max-height 70px
      max-width 70px
      overflow hidden
      margin 10rem 1rem 0 0

      img
        width: 100%
