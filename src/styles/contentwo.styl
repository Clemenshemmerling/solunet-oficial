.sectTwo
  margin-top 1rem
  display flex
  flex-direction column
  color #FAFAFA
  font-family Muli

  &-divOne
    background-color #B3C15D

    &-h1
      font-size 1.5rem
      font-weight normal
      text-align center
      margin-bottom 1rem

    &-pict
      overflow hidden
      img
        display none
    &-p
      margin 1rem 1rem

  &-divTwo
    background-color #19BCBC
    margin-top 1rem


    &-h1
      width 100%
      text-align center
      margin-bottom 0.5rem
      font-size 1.5rem
      font-weight normal

    &-divtext
      display flex

      &-ul
        margin 1rem 0 0 2.5rem
        font-size 1rem

        li
          margin-bottom 0.5rem

      &-picture
        overflow hidden
